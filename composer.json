{
  "name": "mgdsoft/bitbucket-pr-coverage",
  "version": "0.0.1",
  "description": "Create code coverage report in bitbucket",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ptlis/diff-parser": "^1.1.0",
    "symfony/http-client": "^6.0|^7.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/process": "^6.0|^7.0",
    "webmozart/assert": "^1.11",
    "composer-runtime-api": "^2.2"
  },
  "bin": [
    "bin/pr-coverage"
  ],
  "autoload": {
    "psr-4": {
      "BitBucketPRCoverage\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
